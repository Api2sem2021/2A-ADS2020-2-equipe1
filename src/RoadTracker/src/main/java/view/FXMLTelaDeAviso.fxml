<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="150.0" prefWidth="350.0" styleClass="fundoAviso" stylesheets="@designs.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.TelaDeAviso">
   <children> 
      <HBox alignment="TOP_RIGHT" prefHeight="26.0" prefWidth="400.0">
         <children>
            <HBox prefHeight="6.0" prefWidth="367.0">
               <children>
                  <Label prefHeight="27.0" prefWidth="195.0" text="Tela de Avisos" textFill="#bdc3c7">
                     <padding>
                        <Insets left="5.0" />
                     </padding>
                  </Label>
               </children>
            </HBox>
            <Button mnemonicParsing="false" onAction="#fecharJanela" prefHeight="25.0" prefWidth="29.0" styleClass="botaoFechar">
               <graphic>
                  <ImageView fitHeight="17.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@fechar.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
         <styleClass>
            <String fx:value="fundoMenuAviso" />
            <String fx:value="sombraMenuAviso" />
         </styleClass></HBox>
      <Button fx:id="botao" layoutX="92.0" layoutY="98.0" mnemonicParsing="false" onAction="#confirmarAcao" prefHeight="32.0" prefWidth="78.0" styleClass="botaoConfirmar" text="Confirmar" textFill="WHITE" />
      <Button layoutX="232.0" layoutY="98.0" mnemonicParsing="false" onAction="#cancelarAcao" prefHeight="32.0" prefWidth="78.0" styleClass="botaoCancelar" text="Cancelar" textFill="WHITE" />
      <Label alignment="TOP_CENTER" layoutX="57.0" layoutY="32.0" prefHeight="59.0" prefWidth="286.0" text="Aviso" textAlignment="CENTER" textFill="#bdc3c7" wrapText="true" />
   </children>
</AnchorPane>
